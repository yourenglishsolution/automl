<h1>Listing products</h1>

<table>
  <tr>
    <th>Label</th>
    <th>Type</th>
    <th>Lms</th>
    <th>Lms name</th>
    <th>Lms url</th>
    <th>Product ids</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @products.each do |product| %>
  <tr>
    <td><%= product.label %></td>
    <td><%= product.type %></td>
    <td><%= product.lms_id %></td>
    <td><%= product.lms_name %></td>
    <td><%= product.lms_url %></td>
    <td><%= product.product_ids %></td>
    <td><%= link_to 'Show', product_path(product) %></td>
    <td><%= link_to 'Edit', edit_product_path(product) %></td>
    <td><%= link_to 'Destroy', product_path(product), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

